  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/config.git"
  },
  "sideEffects": false,
  "version": "3.14.1",
  "scripts": {
    "package-compile2": "yarn tsc -p tsconfig.json --module es2022 --outDir dist --rootDir src"
