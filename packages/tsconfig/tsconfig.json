{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": [
      "ES2022"
    ],
    "module": "ES2022",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2022",
    "ts-node": {
      "compilerOptions": {
        "module": "CommonJS"
      },
      "files": true,
      "transpileOnly": true
    }
  }
}